-- Content
local Content = Instance.new("ScrollingFrame", Main)
Content.Size = UDim2.new(1,-30,1,-200)
Content.Position = UDim2.new(0,15,0,195)
Content.BackgroundTransparency = 1
Content.ScrollBarThickness = 5
Content.ScrollBarImageColor3 = GetTheme().accent
Content.AutomaticCanvasSize = Enum.AutomaticSize.Y
Content.ZIndex = 5

local Grid = Instance.new("UIGridLayout", Content)
Grid.CellSize = UDim2.new(0.48,0,0,58)
Grid.CellPadding = UDim2.new(0.02,0,0,12)
Grid.HorizontalAlignment = Enum.HorizontalAlignment.Center

local AllButtons = {}

local function AddScriptButton(name, callback)
    local btn = Instance.new("TextButton", Content)
    btn.Size = UDim2.new(1,0,1,0)
    btn.BackgroundColor3 = GetTheme().btn
    btn.Text = name
    btn.TextColor3 = Color3.new(1,1,1)
    btn.TextScaled = true
    btn.FontFace = Font.fromEnum(Enum.Font.GothamBold)
    btn.ZIndex = 7
    Instance.new("UICorner", btn).CornerRadius = UDim.new(0,16)

    local grad = Instance.new("UIGradient", btn)
    grad.Color = ColorSequence.new(GetTheme().accent:lerp(Color3.new(1,1,1),0.3), GetTheme().btn)
    grad.Rotation = 135

    btn.MouseEnter:Connect(function()
        TweenService:Create(btn, TweenInfo.new(0.2), {BackgroundColor3 = GetTheme().btnhover}):Play()
    end)
    btn.MouseLeave:Connect(function()
        TweenService:Create(btn, TweenInfo.new(0.2), {BackgroundColor3 = GetTheme().btn}):Play()
    end)
    btn.MouseButton1Click:Connect(function()
        if callback then spawn(callback) end
    end)

    table.insert(AllButtons, {btn = btn, name = name:lower()})
end

-- === SMOOTH THEME SWITCHING ===
ThemeBtn.MouseButton1Click:Connect(function()
    CurrentTheme = (CurrentTheme % #Themes) + 1
    local t = GetTheme()
    ThemeBtn.Text = "Theme: "..t.name

    -- ULTRA-SMOOTH ANIMATED TRANSITIONS
    TweenService:Create(Glow, TweenInfo.new(1.2, Enum.EasingStyle.Quint), {ImageColor3 = t.glow}):Play()
    TweenService:Create(Main, TweenInfo.new(1.2, Enum.EasingStyle.Quint), {BackgroundColor3 = t.main}):Play()
    TweenService:Create(MainStroke, TweenInfo.new(1.2, Enum.EasingStyle.Quint), {Color = t.accent}):Play()
    TweenService:Create(Welcome, TweenInfo.new(1.2, Enum.EasingStyle.Quint), {TextColor3 = t.accent}):Play()
    TweenService:Create(Title, TweenInfo.new(1.2, Enum.EasingStyle.Quint), {TextColor3 = t.accent}):Play()
    TweenService:Create(SearchStroke, TweenInfo.new(1.2, Enum.EasingStyle.Quint), {Color = t.accent}):Play()
    TweenService:Create(Content, TweenInfo.new(1.2, Enum.EasingStyle.Quint), {ScrollBarImageColor3 = t.accent}):Play()
    TweenService:Create(ThemeBtn, TweenInfo.new(1.2, Enum.EasingStyle.Quint), {BackgroundColor3 = t.accent}):Play()

    -- Animate ALL buttons
    for _, item in ipairs(AllButtons) do
        TweenService:Create(item.btn, TweenInfo.new(1.2, Enum.EasingStyle.Quint), {BackgroundColor3 = t.btn}):Play()
        local grad = item.btn:FindFirstChild("UIGradient")
        if grad then
            TweenService:Create(grad, TweenInfo.new(1.2, Enum.EasingStyle.Quint), {Color = ColorSequence.new(t.accent:lerp(Color3.new(1,1,1),0.3), t.btn)}):Play()
        end
    end
end)

